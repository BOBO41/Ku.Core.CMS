@{
    Layout = "~/Views/Shared/_WindowLayout.cshtml";
}
@using Vino.Core.CMS.Domain.Dto.System
@model SmsQueueDto
<div class="layui-layout layui-layout-window">
    @using (Html.BeginForm<SmsQueueDto>("#"))
    {
    <div class="layui-body">
        @Html.ShowFor(x => x.Status)
        @Html.ShowFor(x => x.Sms.Mobile)
        @Html.ShowFor(x => x.Sms.Content)
        @Html.ShowFor(x => x.CreateTime)
        @Html.ShowFor(x => x.SendTime)
        @Html.ShowFor(x => x.ExpireTime)
    </div>
        <div class="layui-footer">
            @Html.ActionsForClose()
        </div>
    }
</div>